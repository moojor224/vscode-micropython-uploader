{
    "publisher": "moojor224",
    "name": "vscode-micropython-uploader",
    "displayName": "VSCode Micropython Uploader",
    "description": "Upload python files to a connected micropython device",
    "version": "0.0.1",
    "repository": {
        "url": "https://github.com/moojor224/vscode-micropython-uploader/"
    },
    "engines": {
        "vscode": "^1.82.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-micropython-uploader.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.2",
        "@types/node": "18.x",
        "@types/vscode": "^1.82.0",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "@vscode/test-electron": "^2.3.4",
        "@vscode/vsce": "^2.22.0",
        "esbuild": "^0.19.4",
        "eslint": "^8.50.0",
        "glob": "^10.3.3",
        "mocha": "^10.2.0",
        "typescript": "^5.2.2",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@microsoft/vscode-serial-monitor-api": "^0.1.7",
        "linkedom": "^0.15.4",
        "path": "^0.12.7",
        "perf_hooks": "^0.0.1",
        "ts-loader": "^9.5.1",
        "webpack": "^5.89.0"
    }
}
